	display: flex;-webkit-box-orient: horizontal;-webkit-box-direction: normal;-webkit-flex-direction: row;flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	border-bottom: solid 1px #DBDBDA;
	
	padding-bottom: 20rpx;
}
.words-lbl{color: #808080;}
.input{width: 500rpx;margin-left: 40rpx;}
